#
# Nmake file - Windows version
# Use 'nmake -f Makefile.win'

NODEBUG=1

INCS = /I..\libmseed
OPTS = -D_CRT_SECURE_NO_WARNINGS
LIBS = ..\libmseed\libmseed.lib

BIN = ..\sdr2mseed.exe

all: $(BIN)

$(BIN):	decimate.obj sdr2mseed.obj
	link.exe /nologo /out:$(BIN) $(LIBS) decimate.obj sdr2mseed.obj

.c.obj:
	$(CC) /nologo $(CFLAGS) $(INCS) $(OPTS) /c $<

# Clean-up directives
clean:
	-del a.out core *.o *.obj *% *~ $(BIN)
